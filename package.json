{
  "name": "mc-scrpt-live",
  "author": "@cowglow",
  "version": "1.0.0",
  "scripts": {
    "sync": "node -r dotenv/config -r path scripts/sync-resources.js",
    "dev": "svelte-kit dev --host",
    "build": "rm -rf build && svelte-kit build",
    "preview": "svelte-kit preview",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "clean": "rm -r ./build ./.svelte-kit",
    "deploy": "npm run sync && npm run build && gh-pages -t true -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cowglow/mc-scrpt-live.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "^1.0.0-next.278",
    "@types/axios": "^0.14.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "axios": "^0.27.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^3.2.3",
    "prettier": "~2.6.2",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.34.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "type": "module",
  "private": true,
  "dependencies": {}
}
