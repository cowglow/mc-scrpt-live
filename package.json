{
  "name": "mc-scrpt-live",
  "author": "@cowglow",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite dev --port 3000 --host",
    "dev:offline": " php -S localhost:8080 -t private/off-line/",
    "build": "rm -rf build && vite build",
    "preview": "vite preview",
    "sync": "node -r dotenv/config -r path scripts/sync-resources.js",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "clean": "rm -r ./build ./.svelte-kit",
    "deploy": "npm run sync && npm run build && gh-pages -t true -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cowglow/mc-scrpt-live.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.25.0",
    "@sveltejs/adapter-static": "^1.0.0-next.42",
    "@sveltejs/kit": "next",
    "@types/axios": "^0.14.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "axios": "^0.27.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^3.2.3",
    "prettier": "~2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0"
  },
  "type": "module",
  "private": true,
  "dependencies": {}
}
